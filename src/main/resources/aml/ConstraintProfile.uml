      <ownedAttribute xmi:id="_Data-ConstraintProfile-Archetype-other_metadata" name="other_metadata" visibility="public" aggregation="composite">
        <ownedComment xmi:id="_Data-ConstraintProfile-Archetype-other_metadata-ownedComment">
          <body>&lt;html>
  &lt;head>
		&lt;style>
			p {padding:0px; margin:0px;}
		&lt;/style>
	&lt;/head>
  &lt;body>
    &lt;p>
