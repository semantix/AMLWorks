      <ownedRule xmi:id="_Data-ConstraintProfile-ArchetypeRoot-[AOM]_VARXAV-_external_reference_node_archetype_reference_validity" name="[AOM] VARXAV- external reference node archetype reference validity">
        <ownedComment xmi:id="_Data-ConstraintProfile-ArchetypeRoot-[AOM]_VARXAV-_external_reference_node_archetype_reference_validity-ownedComment">
