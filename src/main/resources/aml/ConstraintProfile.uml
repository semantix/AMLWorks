        <type xmi:type="uml:Class" href="pathmap://UML_METAMODELS/UML.metamodel.uml#Classifier"/>
      </ownedAttribute>
    </packagedElement>
    <packagedElement xmi:type="uml:Extension" xmi:id="_Data-ConstraintProfile-packagedElement-18" memberEnd="_Data-ConstraintProfile-ArchetypeSlot-base_Classifier _Data-ConstraintProfile-packagedElement-18-extension_ARCHETYPE_SLOT" navigableOwnedEnd="_Data-ConstraintProfile-packagedElement-18-extension_ARCHETYPE_SLOT">
      <ownedEnd xmi:type="uml:ExtensionEnd" xmi:id="_Data-ConstraintProfile-packagedElement-18-extension_ARCHETYPE_SLOT" name="extension_ARCHETYPE_SLOT" visibility="private" type="_Data-ConstraintProfile-ArchetypeSlot" aggregation="composite" association="_Data-ConstraintProfile-packagedElement-18">
