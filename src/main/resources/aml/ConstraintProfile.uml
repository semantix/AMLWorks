    <packagedElement xmi:type="uml:Stereotype" xmi:id="_Data-ConstraintProfile-ResourceTranslation" name="ResourceTranslation">
      <ownedComment xmi:id="_Data-ConstraintProfile-ResourceTranslation-ownedComment">
